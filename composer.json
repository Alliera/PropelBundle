{
  "name": "propel/propel-bundle",
  "description": "Integration of Propel in Symfony2",
  "keywords": ["propel", "orm", "persistence"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [{
    "name": "William Durand",
    "email": "william.durand1@gmail.com"
  }],
  "require": {
    "symfony/symfony": "~2.2",
    "symfony/propel1-bridge": "~2.2",
    "propel/propel1": "~1.6"
  },
  "require-dev": {
    "sensio/framework-extra-bundle": "~2.2",
    "fzaninotto/faker": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@github.com:Alliera/Propel.git"
    }
  ],
  "conflict": {
      "sensio/framework-extra-bundle": ">=2.4"
  },
  "autoload": {
    "psr-0": { "Propel\\PropelBundle": "" }
  },
  "target-dir": "Propel/PropelBundle",
  "minimum-stability": "dev"
}
